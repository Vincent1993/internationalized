{
  "name": "internationalized",
  "version": "0.0.1",
  "description": "数值格式化组件",
  "keywords": [
    "React",
    "component"
  ],
  "author": "Vincent1993",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "publishConfig": {
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "main": "./dist/index.mjs",
    "exports": {
      "./": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.mjs"
      },
      "./plugins": {
        "types": "./dist/plugins/index.d.ts",
        "import": "./dist/plugins.mjs"
      },
      "./hooks": {
        "types": "./dist/hooks/index.d.ts",
        "import": "./dist/hooks.mjs"
      },
      "./core": {
        "types": "./dist/core/index.d.ts",
        "import": "./dist/core.mjs"
      },
      "./components": {
        "types": "./dist/components/index.d.ts",
        "import": "./dist/components.mjs"
      },
      "./fp": {
        "types": "./dist/fp/index.d.ts",
        "import": "./dist/fp.mjs"
      }
    }
  },
  "exports": {
    ".": "./src/index.ts",
    "./components": "./src/components/index.ts",
    "./fp": "./src/fp/index.ts",
    "./core": "./src/core/index.ts",
    "./hooks": "./src/hooks/index.ts",
    "./plugins": "./src/plugins/index.ts"
  },
  "files": [
    "dist",
    "package.json",
    "README.mdx"
  ],
  "packageManager": "pnpm@10.13.1",
  "dependencies": {
    "big.js": "^7.0.1",
    "d3-format": "^3.1.0"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@rslib/core": "^0.13.3",
    "@rslint/core": "^0.1.13",
    "@rspress/plugin-twoslash": "2.0.0-beta.32",
    "@rspress/plugin-typedoc": "2.0.0-beta.32",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/big.js": "^6.2.2",
    "@types/node": "^22.18.5",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitest/coverage-v8": "^3.2.4",
    "jsdom": "^27.0.0",
    "postcss": "^8.5.6",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "rspress": "2.0.0-beta.21",
    "typescript": "5",
    "vitest": "^3.2.4"
  },
  "scripts": {
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:coverage:ci": "vitest run --coverage --coverage.reporter=text-summary --testNamePattern='^(?!.*缓存应该提高重复调用的性能).*$'",
    "lint": "rslint",
    "lint:fix": "rslint --fix",
    "typecheck": "tsc --noEmit",
    "build": "rslib build",
    "docs:dev": "rspress dev",
    "docs:build": "rspress build",
    "prepublishOnly": "pnpm run build"
  },
  "materials": {
    "framework": "react",
    "category": "component",
    "platform": "pc",
    "tags": [
      "fe-bot"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Vincent1993/internationalized"
  },
  "bugs": "https://github.com/Vincent1993/internationalized/issues"
}
