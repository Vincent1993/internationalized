{
  "name": "internationalized",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@10.13.1",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "pnpm -r run build",
    "test": "vitest run",
    "lint": "pnpm -r run lint",
    "typecheck": "pnpm -r run typecheck",
    "docs:dev": "pnpm --filter @internationalized/number-format run docs:dev",
    "docs:build": "pnpm --filter @internationalized/number-format run docs:build"
  },
  "devDependencies": {
    "@rslib/core": "^0.13.3",
    "@rslint/core": "^0.1.13",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.18.5",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitest/coverage-v8": "^3.2.4",
    "jsdom": "^27.0.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "typescript": "5",
    "vitest": "^3.2.4"
  }
}
